{
  "name": "YC L337 Form",
  "version": "0.0.3",
  "manifest_version": 2,
  "author": "Tom Gabrysiak",
  "description": "This extension is for the L337 only. Use with caution.",
  "homepage_url": "http://tomgabrysiak.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["jquery.js", "src/options_custom/lib/store.js", "src/options_custom/js/i18n.js", "src/options_custom/lib/mootools-core.js"],
      "run_at":"document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "jquery.js","index.js"
  ],
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}